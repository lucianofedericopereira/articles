}

.btn-purple:hover,
.btn-purple.zeroclipboard-is-hover {
    color: var(--white);
    background-color: #5132CB;
    background-image: linear-gradient(#6549D2, #5132CB)
}

.btn-purple:active,
.btn-purple.selected,
.btn-purple.zeroclipboard-is-active {
    background-color: #4F31C6;
