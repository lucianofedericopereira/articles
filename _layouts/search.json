[{% for page in site.pages %}{% if page.url contains '.html' %}
{
    "doc": "{{ page.title | escape | strip }}",
    "title": "{{ page.title | escape | strip }}",
    "content": "{{ page.content | strip_newlines | strip_html | regex_replace: '[^a-zA-Z0-9.,/:;!?]', '' | strip }}",
    "url": "{{ page.url | prepend: site.baseurl | escape }}",
    "relUrl": "{{ page.url | escape }}"
}{% if forloop.last == false %},{% endif %}{% endif %}{% endfor %}
]